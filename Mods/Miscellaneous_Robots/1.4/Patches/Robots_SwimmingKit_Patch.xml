<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>SwimmingKit</li>
      <!-- https://steamcommunity.com/sharedfiles/filedetails/?id=1542399915 -->
      <li>Biomes! Islands</li>
      <!-- https://steamcommunity.com/sharedfiles/filedetails/?id=2038001322 -->
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName = "AIRobot_Hauler"]/statBases</xpath>
          <value>
            <SwimSpeed>4.0</SwimSpeed>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName = "AIRobot_Cleaner"]/statBases</xpath>
          <value>
            <SwimSpeed>0.25</SwimSpeed>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>
