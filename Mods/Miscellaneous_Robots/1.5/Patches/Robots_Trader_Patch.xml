<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	
	<Operation Class="PatchOperationAdd">
		<xpath>*/TraderKindDef[defName="Orbital_Exotic"]/stockGenerators</xpath>
		<value>
			<li Class="StockGenerator_Tag">
				<tradeTag>Robots</tradeTag>
				<thingDefCountRange>
					<min>-1</min>
					<max>2</max>
				</thingDefCountRange>
				<price>Expensive</price>
				<totalPriceRange>
					<min>1500</min>
					<max>5000</max>
				</totalPriceRange>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>Robots</tradeTag>
				<thingDefCountRange>
					<min>-2</min>
					<max>2</max>
				</thingDefCountRange>
				<price>Normal</price>
				<totalPriceRange>
					<min>1200</min>
					<max>3000</max>
				</totalPriceRange>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>Robots</tradeTag>
				<thingDefCountRange>
					<min>-2</min>
					<max>1</max>
				</thingDefCountRange>
				<price>Cheap</price>
				<totalPriceRange>
					<min>1200</min>
					<max>3000</max>
				</totalPriceRange>
			</li>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>*/TraderKindDef[defName="Caravan_Outlander_Exotic"]/stockGenerators</xpath>
