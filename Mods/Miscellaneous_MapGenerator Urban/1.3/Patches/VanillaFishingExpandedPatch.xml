<?xml version="1.0" encoding="utf-8" ?>
<Patch>


<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Fishing Expanded</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeCold"]/biomes</xpath>
				<value>
					<li>BorealForest_UrbanRuins</li>
				</value>
			</li>
			
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeWarm"]/biomes</xpath>
				<value>
					<li>TemperateForest_UrbanRuins</li>
					<li>TemperateSwamp_UrbanRuins</li>
				</value>
			</li>
			
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeHot"]/biomes</xpath>
				<value>
					<li>TropicalRainforest_UrbanRuins</li>
					<li>TropicalSwamp_UrbanRuins</li>
				</value>
			</li>
		</operations>
		</match>
	</Operation>
</Patch>

